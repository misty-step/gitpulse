{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "types": ["jest", "node"],
    "jsx": "preserve",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true
  },
  "include": ["./**/*"],
  "exclude": ["node_modules", "_generated"]
}
