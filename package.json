{
  "name": "gitpulse",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"next dev\" \"npx convex dev\"",
    "build": "pnpm build:app",
    "build:app": "next build",
    "build:local": "npx convex deploy && next build",
    "start": "next start",
    "lint": "eslint app convex components hooks lib tests --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "eval": "promptfoo eval -c evals/promptfooconfig.yaml",
    "eval:view": "promptfoo view",
    "env:check": "node scripts/env-check.mjs",
    "verify:hooks": "scripts/check-hooks.sh",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "convex": "npx convex dev",
    "convex:deploy": "npx convex deploy",
    "reports:generate": "npx convex run actions/reports/regenerate:regenerate",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "prepare": "if [ -d .git ]; then lefthook install; else echo \"Skipping lefthook (no .git)\"; fi"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.45",
    "@clerk/nextjs": "^6.37.3",
    "@openrouter/ai-sdk-provider": "^1.5.2",
    "@sentry/nextjs": "^10.27.0",
    "@stripe/stripe-js": "^8.7.0",
    "ai": "^5.0.108",
    "convex": "^1.29.3",
    "isomorphic-dompurify": "^2.32.0",
    "jsonwebtoken": "^9.0.3",
    "langfuse": "^3.38.6",
    "lucide-react": "^0.561.0",
    "marked": "^12.0.2",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "pino": "^10.1.0",
    "posthog-js": "^1.0.0",
    "posthog-node": "^5.0.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "sonner": "^2.0.7",
    "stripe": "^20.3.1",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@clerk/testing": "^1.13.35",
    "@eslint/eslintrc": "^3.3.3",
    "@jest/globals": "^30.2.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "hono": "4.11.8",
    "import-in-the-middle": "^2.0.0",
    "jest": "29",
    "jest-environment-jsdom": "^30.0.0",
    "jsdom": "^27.2.0",
    "lefthook": "^2.0.4",
    "prettier": "^3.3.3",
    "promptfoo": "^0.120.23",
    "require-in-the-middle": "^8.0.1",
    "tailwindcss": "^4",
    "ts-jest": "29",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=22.22.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0",
  "pnpm": {
    "overrides": {
      "jws": "^4.0.1",
      "qs": ">=6.14.1",
      "js-yaml": ">=4.1.1",
      "fast-xml-parser": ">=5.3.6",
      "axios": ">=1.13.5",
      "tar": ">=7.5.8",
      "minimatch": ">=10.2.1"
    }
  }
}
