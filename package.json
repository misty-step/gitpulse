{
  "name": "gitpulse",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"next dev\" \"npx convex dev\"",
    "build": "npx convex deploy && next build",
    "start": "next start",
    "lint": "eslint app convex components hooks lib tests --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "convex": "npx convex dev",
    "convex:deploy": "npx convex deploy",
    "reports:generate": "npx convex run actions/reports/regenerate:regenerate"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.34.1",
    "convex": "^1.28.0",
    "jsonwebtoken": "^9.0.2",
    "marked": "^12.0.2",
    "next": "16.0.1",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sonner": "^2.0.7",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@jest/globals": "^30.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "jest": "29",
    "prettier": "^3.3.3",
    "tailwindcss": "^4",
    "ts-jest": "29",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=22.15.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}
